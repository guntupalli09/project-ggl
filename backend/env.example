# Environment Variables for LinkedIn OAuth Backend

# Development
NODE_ENV=development
PORT=3001

# LinkedIn OAuth Configuration
LINKEDIN_CLIENT_ID=your_linkedin_client_id_here
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret_here
LINKEDIN_REDIRECT_URI=http://localhost:5173/linkedin/callback

# Frontend URL
FRONTEND_URL=http://localhost:5173

# Production Settings (uncomment for production)
# NODE_ENV=production
# LINKEDIN_REDIRECT_URI=https://www.getgetleads.com/linkedin/callback
# FRONTEND_URL=https://www.getgetleads.com

